{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Idempotency": {
    "DefaultTtlSeconds": 86400,
    "MaxKeysTotal": 10000,
    "MaxKeysPerClient": 1000
  },
  "Outbox": {
    "PollIntervalMs": 1000,
    "BatchSize": 100,
    "LeaseSeconds": 30,
    "MaxAttempts": 5,
    "BaseRetryDelayMs": 500,
    "MaxRetryDelayMs": 30000,
    "MaxParallelism": 4
  },
  "RabbitMQ": {
    "ConfirmTimeoutMs": 5000,
    "MaxChannels": 4,
    "ReconnectBackoffMs": 2000
  }
}
